#include<math.h>
#include<iostream.h>
#include<conio.h>
void main()
{

	int rad=1500;
	clrscr();
	for(double i=10;i<=10;i=i+1)
	{
	double phi=((3.14159)/180)*i;
	for(double j=20;j<=44.005;j=j+10)
	{
	    double theta,x,y,z;
	    theta=((3.14159)/180)*j;
	    x=rad*cos(phi)*cos(theta);
	    y=rad*sin(theta)*cos(phi);
	    z=rad*sin(phi);
	    cout<<x<<"  "<<y<<"  "<<z<<endl;
	}
	}
	getch();
}